<ul class="pl-font">
  <li class="pl-font-karbon-regular">
    [KarbonWeb]: {{ headline.short }}
    {{# meta }}
      {{# font-serif}}
        {{>atoms-scss-style}}
      {{/ font-serif}}
    {{/meta}}
    <hr/>
  </li>
  <li class="pl-font-karbon-regular-italic">
    [KarbonWeb italic]: {{ headline.short }}
    {{# meta }}
      {{# font-serif-italic}}
        {{>atoms-scss-style}}
      {{/ font-serif-italic}}
    {{/meta}}
    <hr/>
  </li>
  <li class="pl-font-karbon-medium">
    [KarbonWeb Medium]: {{ headline.short }}
    {{# meta }}
      {{# font-serif}}
        {{>atoms-scss-style}}
      {{/ font-serif}}
    {{/meta}}
    <hr/>
  </li>
  <li class="pl-font-karbon-medium-italic">
    [KarbonWeb Medium italic]: {{ headline.short }}
    {{# meta }}
      {{# font-serif-italic}}
        {{>atoms-scss-style}}
      {{/ font-serif-italic}}
    {{/meta}}
    <hr/>
  </li>
  <li class="pl-font-karbon-semibold">
    [KarbonWeb semibold]: {{ headline.short }}
    {{# meta }}
      {{# font-serif-bold}}
        {{>atoms-scss-style}}
      {{/ font-serif-bold}}
    {{/meta}}
    <hr/>
  </li>
  <li class="pl-font-karbon-semibold-italic">
    [KarbonWeb semibold italic]: {{ headline.short }}
    {{# meta }}
      {{# font-serif-bold-italic}}
        {{>atoms-scss-style}}
      {{/ font-serif-bold-italic}}
    {{/meta}}
    <hr/>
  </li>
</ul>
